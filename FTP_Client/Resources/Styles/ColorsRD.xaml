<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FF252526" />
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="#FF2D2D30" />
    <SolidColorBrush x:Key="BorderBrush" Color="#FF3F3F41" />
    <SolidColorBrush x:Key="TextBrush" Color="#FFCCCCCC" />
    <SolidColorBrush x:Key="AccentBrush" Color="#FF007ACC" />
    <SolidColorBrush x:Key="AccentHoverBrush" Color="#FF0097FF" />
    <SolidColorBrush x:Key="ErrorBrush" Color="#FFE53935" />
    <SolidColorBrush x:Key="ListItemSelectedBrush" Color="#FF3F3F41" />
    <SolidColorBrush x:Key="ConsoleBackgroundBrush" Color="#FF1E1E1E" />


    <SolidColorBrush x:Key="ScrollBar.Static.Background" Color="#2E2E2E"/>
    <SolidColorBrush x:Key="ScrollBar.Static.Thumb" Color="#555555"/>
    <SolidColorBrush x:Key="ScrollBar.MouseOver.Thumb" Color="#777777"/>
    <SolidColorBrush x:Key="ScrollBar.Pressed.Thumb" Color="#999999"/>

</ResourceDictionary>